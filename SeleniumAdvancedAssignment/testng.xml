<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="classes">
  <test thread-count="5" name="Chrome Test">
    <parameter name="Browser Name" value="Chrome"></parameter>
    <parameter name="URL" value="https://www.lambdatest.com"/>
    <parameter name="Browser Version" value="128.0"/>
    <parameter name="Platform Name" value ="Windows 10"/>
    <classes>
      <class name="advancedSelenium.SeleniumAdvanced"/>
    </classes>
  </test> <!-- Test -->
  <test thread-count="5" name="Microsoft Edge Test">
    <parameter name="Browser Name" value="Microsoft Edge"></parameter>
    <parameter name="URL" value="https://www.lambdatest.com"/>
    <parameter name="Browser Version" value="127.0"/>
    <parameter name="Platform Name" value ="macOS Ventura"/>
    <classes>
      <class name="advancedSelenium.SeleniumAdvanced"/>
    </classes>
  </test>
</suite> <!-- Suite -->
