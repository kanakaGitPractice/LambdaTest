<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="classes">
  <test thread-count="5" name="Chrome Test">
    <parameter name="Browser Name" value="Chrome"></parameter>
    <parameter name="URL" value="https://www.lambdatest.com/selenium-playground/"/>
    <parameter name="Browser Version" value="128.0"/>
    <parameter name="Platform Name" value ="Windows 10"/>
    <classes>
      <class name="com.lambdatest.LambdaTestNGTest"/>
    </classes>
  </test> <!-- Test -->
  
   <test thread-count="5" name="Firefox Test">
    <parameter name="Browser Name" value="Firefox"></parameter>
    <parameter name="URL" value="https://www.lambdatest.com/selenium-playground/"/>
    <parameter name="Browser Version" value="130.0"/>
    <parameter name="Platform Name" value ="Windows 11"/>
    <classes>
      <class name="com.lambdatest.LambdaTestNGTest"/>
    </classes>
  </test>
  <test thread-count="5" name="Microsoft Edge Test">
    <parameter name="Browser Name" value="Microsoft Edge"></parameter>
    <parameter name="URL" value="https://www.lambdatest.com/selenium-playground/"/>
    <parameter name="Browser Version" value="127.0"/>
    <parameter name="Platform Name" value ="macOS Ventura"/>
    <classes>
      <class name="com.lambdatest.LambdaTestNGTest"/>
    </classes>
  </test>
  
  <test thread-count="5" name="Internet Explorer Test">
    <parameter name="Browser Name" value="Internet Explorer"></parameter>
    <parameter name="URL" value="https://www.lambdatest.com/selenium-playground/"/>
    <parameter name="Browser Version" value="11.0"/>
    <parameter name="Platform Name" value ="Windows 10"/>
    <classes>
      <class name="com.lambdatest.LambdaTestNGTest"/>
    </classes>
  </test>
</suite> <!-- Suite -->
